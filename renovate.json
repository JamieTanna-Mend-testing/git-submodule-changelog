{
	"$schema": "https://docs.renovatebot.com/renovate-schema.json",
	"extends": [
		"helpers:githubDigestChangelogs"
	],
	"git-submodules": {
		"enabled": true
	},
	"packageRules": [
		{
			"matchPackageNames": [
				"https://github.com/renovatebot/renovate.git"
			],
			"matchManagers": [
				"git-submodules"
			],
			"sourceUrl": "https://github.com/renovatebot/renovate.git"
		}
	]
}
